# Copy to .env and fill in values. Do not commit .env.
# Backend runs from backend/ or project root; .env is loaded from backend/ or root.

# PostgreSQL connection string (e.g. postgresql://user:pass@localhost:5432/case_agent)
DATABASE_URL=

# OpenAI API key for agents and embeddings
OPENAI_API_KEY=

# Tavily Search API key for research agent
TAVILY_API_KEY=

# Secret key for JWT signing (min 32 characters)
SECRET_KEY=

# Frontend origin(s) for CORS (comma-separated; e.g. http://localhost:5173)
FRONTEND_URL=

# Environment: development, staging, or production
ENVIRONMENT=development
